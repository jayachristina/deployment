[all:vars]

ocp4_workload_connectivity_link_aws_access_key=<your key>
ocp4_workload_connectivity_link_aws_secret_access_key=<your secret>

# E.g.: sandbox902.opentlc.com
ocp4_workload_connectivity_link_main_domain=<your domain>

#don't change unless mandatory
ocp4_workload_connectivity_link_sub_domain_name=globex

# E.g.: This is the region where you have OpenShift running. (example eu-central-1)
ocp4_workload_connectivity_link_aws_managed_zone_region=<your region>

#your email address
ocp4_workload_connectivity_link_ingress_gateway_tls_issuer_email=<your email>

#E.g. EU/US
ocp4_workload_connectivity_link_gateway_geo_code=GEO-NA
